{
  "name": "shared",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:package": "tsdown --config tsdown.config.ts",
    "build:watch": "tsdown --config tsdown.config.ts --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "clean:full": "rm -rf dist node_modules"
  },
  "devDependencies": {
    "@types/dependency-tree": "^8.1.4",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.18.1",
    "dependency-tree": "^11.2.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json"
    }
  },
  "dependencies": {
    "@ast-grep/napi": "^0.37.0",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.3"
  }
}
