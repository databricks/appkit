{
  "name": "@databricks/app-kit-ui",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.21.0",
  "files": [
    "dist",
    "llms.txt",
    "README.md"
  ],
  "exports": {
    "./js": {
      "development": "./src/js/index.ts",
      "default": "./dist/js/index.js"
    },
    "./react": {
      "development": "./src/react/index.ts",
      "default": "./dist/react/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:package": "tsdown --config tsdown.config.ts",
    "build:watch": "tsdown --config tsdown.config.ts --watch",
    "dist": "tsx ../../tools/dist.ts && npm pack ./tmp --pack-destination ./tmp",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist tmp",
    "clean:full": "rm -rf dist node_modules tmp"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.554.0",
    "shared": "workspace:*",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "recharts": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.4.1"
  },
  "publishConfig": {
    "exports": {
      "./js": "./dist/js/index.js",
      "./react": "./dist/react/index.js",
      "./package.json": "./package.json"
    }
  }
}
